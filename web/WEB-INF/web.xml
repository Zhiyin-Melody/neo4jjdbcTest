<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>SeverletTest</servlet-name>
        <servlet-class>com.zhiyin.SeverletTest</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SeverletTest</servlet-name>
        <url-pattern>/SeverletTest</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>SeverletQuery</servlet-name>
        <servlet-class>main.java.SeverletQuery</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SeverletQuery</servlet-name>
        <url-pattern>/SeverletQuery</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>